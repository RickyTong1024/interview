#include <iostream>
#include <vector>

using namespace std;

class Solution {
public:
    int pack(vector<vector<int>>& boxes) {
        
    }
};

int main() {
	int n;
	cin >> n;
	vector<vector<int>> boxes;
	int crow;
	for (int i = 0; i < n; ++i) {
		vector<int> vrow;
		for (int j = 0; j < 3; ++j) {
			cin >> crow;
			vrow.push_back(crow);
		}
		boxes.push_back(vrow);
	}
	Solution solution;
	int out_num = solution.pack(boxes);
	cout << out_num << endl;
	return 0;
}

import java.io.*;
import java.util.*;

class Solution {
	public int add(int a, int b) {
		return a + b;
	}
}

public class Main {
	public static void main(String[] args) throws IOException {
		Scanner cin = new Scanner(System.in);
		int a = cin.nextInt();
		int b = cin.nextInt();
		cin.close();
		Solution solution = new Solution();
		int out_num = solution.add(a, b);
		System.out.println(out_num);
	}
}
